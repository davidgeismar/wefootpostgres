      <section id="main-content">
          <section class="wrapper">
              <!-- page start-->
              <div class="row">
                <div class="col-sm-12">
              <section class="panel">
              <header class="panel-heading">
                  Tables des Users
             <span class="tools pull-right">
                <a href="javascript:;" class="fa fa-chevron-down"></a>
             </span>
              </header>
              <div class="panel-body">
              <div class="adv-table">
              <table  class="display table table-bordered table-striped" id="dynamic-table">
              <thead>
              <tr>
                  <th>ID</th>
                  <th>Nom</th>
                  <th>Prénom</th>
                  <th>Email</th>
                  <th>Inscrit le</th>
                  <th>Nombre de connexions</th>
                  <th class="hidden-phone">Facebook</th>
                  <th class="hidden-phone">Foot Créés</th>
                  <th class="hidden-phone">Foot Joués</th>
                  <th class="hidden-phone">Online</th>
              </tr>
              </thead>
              <tbody>
              <% _.each(users,function(user){%>
                <tr class="gradeX">
                    <td><%=user.id%></td>
                    <td><%=user.first_name%></td>
                    <td><%=user.last_name%></td>
                    <td><%= user.email%></td>
                    <td><%=moment(user.createdAt).locale('fr').format('L')%></td>
                    <td><%=user.nb_connection%> </td>
                    <td class="center hidden-phone"><%=user.facebook_id%></td>
                    <td class="center hidden-phone"><%=user.created_foot%></td>
                    <td class="center hidden-phone"><%= user.played_foot %></td>
                    <td class="center hidden-phone"><%= user.connexion%></td>
                </tr>
              <%})%>
              </tfoot>
              </table>
              </div>
              </div>
              </section>
              </div>
              </div>



                      <div class="row">
                <div class="col-sm-12">
              <section class="panel">
              <header class="panel-heading">
                  Tables des Foots
             <span class="tools pull-right">
                <a href="javascript:;" class="fa fa-chevron-down"></a>
             </span>
              </header>
              <div class="panel-body">
              <div class="adv-table">
              <table  class="display table table-bordered table-striped" id="dynamic-table1">
              <thead>
              <tr>
                  <th>ID</th>
                  <th>NB Joueur</th>
                  <th>Nom terrain</th>
                  <th>Créé le</th>
                  <th>Date du match</th>
              </tr>
              </thead>
              <tbody>
              <% _.each(foots,function(foot){%>
                <tr class="gradeX">
                    <td><%=foot.id%></td>
                    <td><%=foot.nb_player%></td>
                    <td><%=foot.fieldName%></td>
                    <td><%=moment(foot.createdAt).locale('fr').format('L')%></td>
                    <td><%=moment(foot.date).locale('fr').format('L')%> </td>
                </tr>
              <%})%>
              </tfoot>
              </table>
              </div>
              </div>
              </section>
              </div>
              </div>


                <div class="row">
                <div class="col-sm-12">
              <section class="panel">
              <header class="panel-heading">
                  Tables des Reservations
             <span class="tools pull-right">
                <a href="javascript:;" class="fa fa-chevron-down"></a>
             </span>
              </header>
              <div class="panel-body">
              <div class="adv-table">
              <table  class="display table table-bordered table-striped" id="dynamic-table2">
              <thead>
              <tr>
                  <th>ID</th>
                  <th>ID Joueur</th>
                  <th>Nom terrain</th>
                  <th>Créé le</th>
                  <th>Date du match</th>
              </tr>
              </thead>
              <tbody>
              <% _.each(resas,function(resa){%>
                <tr class="gradeX">
                    <td><%=resa.id%></td>
                    <td><%=resa.user%></td>
                    <td><%=resa.fieldName%></td>
                    <td><%=moment(resa.createdAt).locale('fr').format('L')%></td>
                    <td><%=moment(resa.date).locale('fr').format('L')%> </td>
                </tr>
              <%})%>
              </tfoot>
              </table>
              </div>
              </div>
              </section>
              </div>
              </div>
              <!-- page end-->
          </section>
      </section>
    <script src="/scripts/jquery.js"></script>
    <script src="/scripts/jquery-ui-1.9.2.custom.min.js"></script>
    <script src="/scripts/jquery-migrate-1.2.1.min.js"></script>
    <script src="/scripts/bootstrap.min.js"></script>
